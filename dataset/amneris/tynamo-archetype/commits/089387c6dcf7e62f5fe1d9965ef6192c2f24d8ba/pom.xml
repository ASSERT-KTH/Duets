<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>org.amneris</groupId>
	<artifactId>tynamo-archetype</artifactId>
	<version>0.4.0-SNAPSHOT</version>
	<name>tynamo-archetype</name>
	<description>Archetype for creating a model-driven, full-stack Tynamo application</description>
	<url>https://github.com/amneris/tynamo-archetype</url>
	<packaging>maven-archetype</packaging>

	<scm>
		<connection>scm:git:git://github.com/amneris/tynamo-archetype.git</connection>
		<developerConnection>scm:git:git@github.com:amneris/tynamo-archetype.git</developerConnection>
		<url>https://github.com/amneris/tynamo-archetype/tree/master/</url>
	</scm>

	<build>

		<extensions>
			<extension>
				<groupId>org.apache.maven.archetype</groupId>
				<artifactId>archetype-packaging</artifactId>
				<version>2.2</version>
			</extension>
		</extensions>

		<plugins>
			<plugin>
				<artifactId>maven-archetype-plugin</artifactId>
				<version>2.2</version>
				<extensions>true</extensions>
			</plugin>
		</plugins>
	</build>

</project>