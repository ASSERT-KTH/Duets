<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.maximuspayne</groupId>
	<artifactId>NavyCraft</artifactId>
	<version>2.0.0-SNAPSHOT</version>
	<name>NavyCraft</name>
	<description>a naval plugin</description>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<mainClass>NavyCraft</mainClass>
		<author>Pocketkid2 and Keough99</author>
	</properties>
	<build>
		<resources>
			<resource>
				<directory>${basedir}/src/main/resources</directory>
				<filtering>true</filtering>
				<includes>
					<include>plugin.yml</include>
				</includes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<finalName>${project.name}</finalName>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<url>https://github.com/Dabblecraft2DevTeam/NavyCraft</url>
	<dependencies>
	<dependency>
		        <groupId>org.bukkit</groupId>
			<artifactId>bukkit</artifactId>
			<version>LATEST</version>
			<systemPath>${project.basedir}/jars/craftbukkit-1.12.2.jar</systemPath>
			<scope>system</scope>
	</dependency>
	<dependency>
        		<groupId>com.sk89q</groupId>
        		<artifactId>worldguard</artifactId>
        		<version>LATEST</version>
			<systemPath>${project.basedir}/jars/worldguard-legacy-6.2.2-SNAPSHOT-dist.jar</systemPath>
                        <scope>system</scope>
        </dependency>
	<dependency>
        	<groupId>com.sk89q</groupId>
        	<artifactId>worldedit</artifactId>
        	<version>LATEST</version>
		<systemPath>${project.basedir}/jars/worldedit-bukkit-6.1.9-SNAPSHOT-dist.jar</systemPath>
                <scope>system</scope>
        </dependency>
	<dependency>
           	 <groupId>net.ess3</groupId>
           	 <artifactId>Essentials</artifactId>
           	 <version>LATEST</version>
		 <systemPath>${project.basedir}/jars/Essentials-2.x-SNAPSHOT.jar</systemPath>
                <scope>system</scope>
        </dependency>
	<dependency>
            	<groupId>com.comphenix</groupId>
           	 <artifactId>ProtocolLib</artifactId>
           	 <version>LATEST</version>
           	 <systemPath>${project.basedir}/jars/ProtocolLib.jar</systemPath>
                <scope>system</scope>
        </dependency>
	<dependency>
           	 <groupId>org.anjocadio</groupId>
           	 <artifactId>EssesntialsGroupManager</artifactId>
            	<version>LATEST</version>
          	<systemPath>${project.basedir}/jars/EssentialsGroupManager2.14.57.jar</systemPath>
                <scope>system</scope>
        </dependency>
	<dependency>
           	 <groupId>com.bergerkiller</groupId>
           	 <artifactId>BKCommonLib</artifactId>
            	<version>LATEST</version>
          	<systemPath>${project.basedir}/jars/BKCommonLib.jar</systemPath>
                <scope>system</scope>
        </dependency>
	</dependencies>
</project>
