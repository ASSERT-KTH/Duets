<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>oss-parent</artifactId>
    <groupId>org.sonatype.oss</groupId>
    <version>7</version>
  </parent>

  <groupId>org.agilewiki.jactor</groupId>
  <artifactId>jactor-in-action</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>JActor-In-Action</name>
  <description>JActor code samples</description>
  <url>https://github.com/laforge49/JActor</url>


  <dependencies>
      <dependency>
          <groupId>org.agilewiki.jactor</groupId>
          <artifactId>jactor</artifactId>
          <version>3.2.0</version>
      </dependency>
  </dependencies>

  <scm>
      <connection>scm:git:git@github.com:anirbanbhattacharjee/jactor-in-action.git</connection>
      <developerConnection>scm:git:git@github.com:anirbanbhattacharjee/jactor-in-action.git</developerConnection>
      <url>git@github.com:anirbanbhattacharjee/jactor-in-action.git</url>
  </scm>

   <developers>
     <developer>
         <id>laforge49</id>
         <name>Bill La Forge</name>
         <email>laforge49@gmail.com</email>
     </developer>
     <developer>
         <id>anirbanbhattacharjee</id>
         <name>Anirban Bhattacharjee</name>
         <email>anirban.bhattacharjee@gmail.com</email>
     </developer>
    </developers>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.0.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.1.2</version>
                <configuration>
                    <includePom>true</includePom>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>