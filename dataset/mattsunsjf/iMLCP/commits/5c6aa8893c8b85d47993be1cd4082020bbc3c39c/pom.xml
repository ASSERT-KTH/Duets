  <groupId>imlcp</groupId>
  <artifactId>imlcp</artifactId>
  <version>8.03</version>
  <packaging>jar</packaging>

  <name>imlcp</name>
  <url>http://maven.apache.org</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <repositories>
    <repository>
      <id>MarkLogic-releases</id>
      <name>MarkLogic Releases</name>
      <url>http://developer.marklogic.com/maven2</url>
  </repository>
  </repositories>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <version>2.10</version>
        <executions>
          <execution>
            <id>copy</id>
            <phase>package</phase>
            <goals>
              <goal>copy</goal>
            </goals>
            <configuration>
              <artifactItems>
              	<artifactItem>
                  <groupId>jlineplus</groupId>
                  <artifactId>jlineplus</artifactId>
                  <version>2.15</version>
                  <overWrite>true</overWrite>
                  <outputDirectory>${basedir}/target</outputDirectory>
                </artifactItem>
              </artifactItems>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

