<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.renemoser.libjsms</groupId>
  <artifactId>LibJSMS</artifactId>
  <packaging>jar</packaging>
  <version>0.3</version>
  <name>LibJSMS</name>
  <description>Java library to send short messages (alias SMS) using free web service of your mobile phone provider. Currently only Swiss mobile providers are supported, but feel free to add you provider's web interface.</description>
  <url>http://www.renemoser.net/project</url>
  <issueManagement>
  	<system>GitHub Issues</system>
  	<url>http://github.com/resmo/libjsms/issues</url>
  </issueManagement>
  <developers>
  	<developer>
  		<id>resmo</id>
  		<name>Ren√© Moser</name>
  		<email>mail@renemoser.net</email>
  		<url>http://www.renemoser.net</url>
  		<roles>
  			<role>Developer</role>
  		</roles>
  	</developer>
  </developers>
  <contributors>
  </contributors>
 <build>
	<plugins>
	 <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
</plugins>
</build>
<dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
