<?xml version="1.0" encoding="UTF-8"?>

<project>
  <modelVersion>4.0.0</modelVersion>
  
  <groupId>net.meisen.dissertation</groupId>
  <artifactId>net-meisen-dissertation-dis-timeintervaldataanalyzer</artifactId>
  <version>TRUNK-SNAPSHOT</version>
  <name>dis-timeintervaldataanalyzer</name>
  <description>Time Interval Data Analyzer</description>
  <packaging>jar</packaging>
  
  <dependencies>
    <dependency>
      <groupId>net.meisen.general</groupId>
      <artifactId>net-meisen-general-gen-misc</artifactId>
      <version>TRUNK-SNAPSHOT</version>
    </dependency>
    
    <!-- add the driver for the common protocol -->
    <dependency>
      <groupId>net.meisen.dissertation</groupId>
      <artifactId>net-meisen-dissertation-dis-timeintervaldataanalyzer-jdbc</artifactId>
      <version>TRUNK-SNAPSHOT</version>
    </dependency>
    
    <!-- ConnectionManager -->
    <dependency>
      <groupId>com.zaxxer</groupId>
      <artifactId>HikariCP-java6</artifactId>
      <version>2.2.5</version>
    </dependency>

    <!-- JSON for servlets -->
    <dependency>
      <groupId>com.eclipsesource.minimal-json</groupId>
      <artifactId>minimal-json</artifactId>
      <version>0.9.1</version>
    </dependency>
    
    <!-- FileUpload -->
    <dependency>
      <groupId>commons-fileupload</groupId>
      <artifactId>commons-fileupload</artifactId>
      <version>1.3</version>
    </dependency>
    
    <!-- security, i.e. authentication, authorization, ... -->
    <dependency>
      <groupId>org.apache.shiro</groupId>
      <artifactId>shiro-core</artifactId>
      <version>1.2.3</version>
    </dependency>
    
    <!-- server -->
    <dependency>
      <groupId>net.meisen.general</groupId>
      <artifactId>net-meisen-general-server</artifactId>
      <version>TRUNK-SNAPSHOT</version>
    </dependency>
    
    <dependency>
      <groupId>net.meisen.general</groupId>
      <artifactId>net-meisen-general-server-http-listener</artifactId>
      <version>TRUNK-SNAPSHOT</version>
    </dependency>
    
    <!-- indexes -->
    <dependency>
      <groupId>it.unimi.dsi</groupId>
      <artifactId>fastutil</artifactId>
      <version>6.5.11</version>
      <optional>true</optional>
    </dependency>
    
    <dependency>
      <groupId>com.carrotsearch</groupId>
      <artifactId>hppc</artifactId>
      <version>0.5.3</version>
      <optional>true</optional>
    </dependency>
    
    <dependency>
      <groupId>net.sf.trove4j</groupId>
      <artifactId>trove4j</artifactId>
      <version>3.0.3</version>
    </dependency>
    
    <!-- advanced bitset -->
    <dependency>
      <groupId>com.googlecode.javaewah</groupId>
      <artifactId>JavaEWAH</artifactId>
      <version>0.8.2</version>
      <optional>true</optional>
    </dependency>
    
    <dependency>
      <groupId>org.roaringbitmap</groupId>
      <artifactId>RoaringBitmap</artifactId>
      <version>0.4.3</version>
    </dependency>
    
    <!-- 
      Internally used database for users, additionally
      a cache implementation using mapDb is available.
      --> 
    <dependency>
      <groupId>org.mapdb</groupId>
      <artifactId>mapdb</artifactId>
      <version>1.0.3</version>
    </dependency>

    <!-- the old JavaScript engine, prior Java 8, use 'rhino' as engine -->
    <dependency>
      <groupId>cat.inspiracio</groupId>
      <artifactId>rhino-js-engine</artifactId>
      <version>1.7.7.1</version>
      <optional>true</optional>
    </dependency>
    
    <!-- parser -->
    <dependency>
      <groupId>org.antlr</groupId>
      <artifactId>antlr4-runtime</artifactId>
      <version>4.2</version>
      
      <!-- remove the treelayout it's only used in the GUI -->
      <exclusions>
        <exclusion>
          <groupId>org.abego.treelayout</groupId>
          <artifactId>org.abego.treelayout.core</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    
    <!-- logging -->
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-log4j12</artifactId>
      <version>1.7.2</version>
    </dependency>
    
    <!-- tests -->
    <dependency>
      <groupId>net.meisen.general</groupId>
      <artifactId>net-meisen-general-gen-sbconfigurator</artifactId>
      <version>TRUNK-SNAPSHOT</version>
      <classifier>test</classifier>
      <scope>test</scope>
    </dependency>
    
    <dependency>
      <groupId>org.hsqldb</groupId>
      <artifactId>hsqldb</artifactId>
      <version>2.3.2</version>
      <scope>test</scope>
    </dependency>
    
    <!-- used for tests on HTTP -->
    <dependency>
      <groupId>org.apache.httpcomponents</groupId>
      <artifactId>httpclient</artifactId>
      <version>4.3.2</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.httpcomponents</groupId>
      <artifactId>httpmime</artifactId>
      <version>4.3.2</version>
      <scope>test</scope>
    </dependency>
    
    <dependency>
      <groupId>org.mockito</groupId>
      <artifactId>mockito-all</artifactId>
      <version>1.9.5</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>com.jolbox</groupId>
      <artifactId>bonecp</artifactId>
      <version>0.8.0.RELEASE</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>