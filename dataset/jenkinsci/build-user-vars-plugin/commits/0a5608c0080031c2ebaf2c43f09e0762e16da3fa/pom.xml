    </licenses>

    <developers>
        <developer>
            <id>gkonovalenko</id>
            <name>Gavriil Konovalenko</name>
            <email>gkonovalenko@gmail.com</email>
        </developer>
    </developers>

    <dependencies>
        <dependency>
            <groupId>org.jenkins-ci.plugins</groupId>
