<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.jenkins-ci.plugins</groupId>
    <artifactId>plugin</artifactId>
    <version>1.399</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <artifactId>perfpublisher</artifactId>
  <packaging>hpi</packaging>
  <version>8.01</version>
  <name>Jenkins Performance Publisher plugin</name>
  <description>The PerfPublisher plug-in scans for xml tests reports defined in the administration panel and generates trend graphs, computes stats and underline regressions and modifications. The xml files must be generated by your own testing tool.</description>
  <url>http://wiki.jenkins-ci.org/display/JENKINS/PerfPublisher+Plugin</url>


  <licenses>
    <license>
      <name>MIT license</name>
      <comments>All source code is under the MIT license.</comments>
    </license>
  </licenses>

  <dependencies>
    <dependency>
      <groupId>org.jvnet.hudson.plugins</groupId>
      <artifactId>analysis-core</artifactId>
      <version>1.1</version>
    </dependency>

    <dependency>
      <groupId>org.jvnet.hudson.plugins</groupId>
      <artifactId>analysis-test</artifactId>
      <version>1.0</version>
      <scope>test</scope>
    </dependency>
    
    <dependency>
    	<groupId>net.sourceforge.jexcelapi</groupId>
    	<artifactId>jxl</artifactId>
    	<version>2.6.12</version>
    </dependency>
  </dependencies>

  <developers>
    <developer>
      <name>Georges Bossert</name>
      <email>gbossert@gmail.com</email>
      <roles>
  	<role>Developer</role>
      </roles>
    </developer>
    <developer>
      <name>Dmitri Makarov</name>
      <email>dmakarv@gmail.com</email>
      <roles>
  	    <role>Developer</role>
      </roles>
    </developer>
    <developer>
      <id>martinbaar</id>
      <name>Martin Baar</name>
      <email>martin.baar@gmail.com</email>
      <roles>
  	    <role>Developer</role>
      </roles>
    </developer>
  </developers>

  <organization>
  </organization>

  <build>
    <resources>
    </resources>
    <plugins>
      <plugin>
        <groupId>org.kohsuke.stapler</groupId>
        <artifactId>maven-stapler-plugin</artifactId>
        <extensions>true</extensions>
      </plugin>
      <plugin>
        <groupId>org.jvnet.hudson.tools</groupId>
        <artifactId>maven-hpi-plugin</artifactId>
        <configuration>
          <showDeprecation>true</showDeprecation>
          <showWarnings>true</showWarnings>
        </configuration>
      </plugin>
    </plugins>
  </build>
  
  <scm>
    <connection>scm:git:git://github.com/jenkinsci/perfpublisher-plugin.git</connection>
    <developerConnection>scm:git:git@github.com:jenkinsci/perfpublisher-plugin.git</developerConnection>
    <url>http://github.com/jenkinsci/perfpublisher-plugin</url>
  </scm>

  <repositories>
    <!-- We put the central repository first, as most artifacts will be found there -->
    <repository>
      <id>java.net2</id>
      <url>http://download.java.net/maven/2/</url>
      <releases>
        <enabled>true</enabled>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>repo.jenkins-ci.org</id>
      <url>http://repo.jenkins-ci.org/public/</url>
    </repository>
  </repositories>

  <pluginRepositories>
    <!-- We put the central repository first, as most artifacts will be found there -->
    <pluginRepository>
      <id>central</id>
      <url>http://repo1.maven.org/maven2</url>
      <releases>
        <enabled>true</enabled>
        <!-- only look for jars here when they are not present locally -->
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </pluginRepository>
    <pluginRepository>
      <id>java.net2</id>
      <url>http://download.java.net/maven/2</url>
      <releases>
        <enabled>true</enabled>
        <!-- only look for jars here when they are not present locally -->
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </pluginRepository>
    <pluginRepository>
            <id>repo.jenkins-ci.org</id>
            <url>http://repo.jenkins-ci.org/public/</url>
    </pluginRepository>
  </pluginRepositories>

</project>
