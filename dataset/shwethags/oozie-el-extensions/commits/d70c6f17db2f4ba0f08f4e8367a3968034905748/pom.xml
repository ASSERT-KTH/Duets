<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.apache.oozie.extensions</groupId>
  <artifactId>oozie-el-extensions</artifactId>
  <version>0.1-SNAPSHOT</version>
  <name>Oozie Coordinator EL extensions</name>
  <url>https://github.com/shwethags/oozie-el-extensions</url>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <internal.maven.repository>http://build.corp.inmobi.com/nexus/content/repositories/snapshots</internal.maven.repository>
  </properties>
    <distributionManagement>
        <repository>
            <id>InMobiRepository</id>
            <name>InMobi Internal Repository</name>
            <url>http://build.corp.inmobi.com/nexus/content/repositories/snapshots</url>
        </repository>
    </distributionManagement>
  <repositories>
    <repository>
      <id>internal.repo</id>
      <url>${internal.maven.repository}</url>
    </repository>
    <repository>
      <id>cdh.repo</id>
      <url>https://repository.cloudera.com/content/groups/cloudera-repos</url>
      <name>Cloudera Repository</name>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.apache.hadoop</groupId>
      <artifactId>hadoop-core</artifactId>
      <version>0.20.2-cdh3u0</version>
    </dependency>
    <dependency>
      <groupId>org.apache.oozie</groupId>
      <artifactId>oozie-core</artifactId>
      <version>3.1.3-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.testng</groupId>
      <artifactId>testng</artifactId>
      <version>5.8</version>
      <classifier>jdk15</classifier>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
