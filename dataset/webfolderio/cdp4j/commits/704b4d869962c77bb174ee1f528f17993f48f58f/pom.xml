    <profile>
      <id>native</id>
      <build>
        <plugins>
          <plugin>
            <groupId>org.graalvm.nativeimage</groupId>
            <artifactId>native-image-maven-plugin</artifactId>
            <version>20.1.0</version>
            <executions>
              <execution>
                <goals>
                  <goal>native-image</goal>
                </goals>
                <phase>package</phase>
              </execution>
            </executions>
            <configuration>
              <mainClass>io.webfolder.cdp.HelloGraal</mainClass>
              <imageName>cdp4j-native</imageName>
              <!-- HelloGraal use default WebSocketFactory which is JreWebSocketFactory.                     -->
              <!-- JreWebSocketFactory requires http and https features of native-image.                     -->
              <!-- This option might be not required for other WebSocketFactory  implementations.            -->
              <!-- enable-http and enable-https increase the native-image size.                              -->
              <!-- You might try another WebSocketFactory implementation to decrease  the native-image size. -->
              <buildArgs>-H:+ReportExceptionStackTraces --no-fallback --verbose</buildArgs>
            </configuration>
          </plugin>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>build-helper-maven-plugin</artifactId>
            <version>3.1.0</version>
            <executions>
              <execution>
