<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.tupilabs</groupId>
  <artifactId>pbs-java-api</artifactId>
  <version>0.11-SNAPSHOT</version>
  <name>PBS Java API</name>
  <description>An API written in Java for interfacing with PBS clusters</description>
  <url>https://github.com/biouno/pbs-java-api</url>
  <licenses>
    <license>
      <name>MIT License</name>
      <url>http://www.opensource.org/licenses/mit-license.php</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>kinow</id>
      <email>brunodepaulak@yahoo.com.br</email>
      <name>Bruno P. Kinoshita</name>
      <timezone>-3</timezone>
      <url>http://www.kinoshita.eti.br</url>
      <organization>TupiLabs</organization>
      <organizationUrl>http://www.tupilabs.com</organizationUrl>
      <roles>
        <role>developer</role>
      </roles>
    </developer>
    <developer>
      <id>liccini</id>
      <email>joseph.liccini@ufl.edu</email>
      <name>Joseph Liccini</name>
      <timezone>-5</timezone>
      <organization>RAPiD Genomics</organization>
      <organizationUrl>http://www.rapid-genomics.com</organizationUrl>
      <roles>
        <role>developer</role>
      </roles>
    </developer>
  </developers>
  <scm>
    <url>https://github.com/biouno/pbs-java-api</url>
    <connection>scm:git:git://github.com/biouno/pbs-java-api.git</connection>
    <developerConnection>scm:git:git@github.com:biouno/pbs-java-api.git</developerConnection>
    <tag>HEAD</tag>
  </scm>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <java.version>1.6</java.version>
  </properties>
  <inceptionYear>2012</inceptionYear>
  <organization>
    <name>TupiLabs</name>
    <url>http://www.tupilabs.com</url>
  </organization>
  <issueManagement>
    <url>https://github.com/biouno/pbs-java-api/issues</url>
    <system>GitHub</system>
  </issueManagement>
  <ciManagement>
    <url>https://buildhive.cloudbees.com/job/biouno/job/pbs-java-api/</url>
    <system>Jenkins</system>
  </ciManagement>
  <dependencies>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>commons-exec</artifactId>
      <version>1.1</version>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>2.6</version>
    </dependency>
    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
      <version>2.3</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.10</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>${java.version}</source>
          <target>${java.version}</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.19.1</version>
        <configuration />
      </plugin>
      <plugin>
        <groupId>org.jacoco</groupId>
        <artifactId>jacoco-maven-plugin</artifactId>
        <version>0.8.5</version>
        <executions>
          <execution>
            <goals>
              <goal>prepare-agent</goal>
            </goals>
          </execution>
          <execution>
            <id>report</id>
            <phase>test</phase>
            <goals>
              <goal>report</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
