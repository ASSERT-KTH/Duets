                <datasource.username>root</datasource.username>
                <datasource.password>123456</datasource.password>
                <druid.initial-size>1</druid.initial-size>
                <druid.min-idle>3</druid.min-idle>
                <druid.max-active>20</druid.max-active>
                <druid.max-wait>60000</druid.max-wait>
                <druid.min-evictable-idle-time-millis>30000</druid.min-evictable-idle-time-millis>
                <druid.time-between-eviction-runs-millis>60000</druid.time-between-eviction-runs-millis>
                <druid.validation-query>SELECT 'x' FROM DUAL</druid.validation-query>
                <druid.test-while-idle>true</druid.test-while-idle>
                <druid.test-on-borrow>true</druid.test-on-borrow>
                <druid.test-on-return>true</druid.test-on-return>
                <druid.pool-prepared-statements>true</druid.pool-prepared-statements>
