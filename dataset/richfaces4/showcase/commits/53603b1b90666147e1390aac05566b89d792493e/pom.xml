        <profile>
            <id>unpack-gae-sdk</id>
            <activation>
                <property>
                    <name>!gae.home</name>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>com.google.appengine</groupId>
                    <artifactId>appengine-java-sdk</artifactId>
                    <version>${gae.version}</version>
                    <type>zip</type>
                </dependency>
