<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <groupId>com.twitter.diagnostics</groupId>
  <artifactId>PerfTracepoint</artifactId>
  <version>1.0.0</version>
  <packaging>jar</packaging>

  <name>PerfTracepoint</name>
  <description>Perf tracepoint support for the JVM.</description>
  <url>http://github.com/twitter/PerfTracepoint</url>

  <licenses>
    <license>
      <name>The Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>

  <developers>
    <developer>
      <name>Kaushik Srenevasan</name>
      <email>kaushik@twitter.com</email>
      <organization>Twitter</organization>
      <organizationUrl>http://twitter.com/</organizationUrl>
    </developer>
  </developers>

  <scm>
    <connection>scm:git:git@github.com:twitter/PerfTracepoint.git</connection>
    <url>scm:git:git@github.com:twitter/PerfTracepoint.git</url>
    <developerConnection>scm:git:git@github.com:twitter/PerfTracepoint.git</developerConnection>
  </scm>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.0</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
