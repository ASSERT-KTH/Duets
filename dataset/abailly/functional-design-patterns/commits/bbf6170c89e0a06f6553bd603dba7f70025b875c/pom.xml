    <build>
        <plugins>
            <plugin>
                <groupId>
                    org.apache.maven.plugins
                </groupId>
                <artifactId>
                    maven-compiler-plugin
                </artifactId>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                    <encoding>iso-8859-1</encoding>
                </configuration>
            </plugin>

        </plugins>
    </build>

