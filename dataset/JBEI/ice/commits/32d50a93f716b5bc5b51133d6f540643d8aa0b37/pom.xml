<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.jbei</groupId>
    <artifactId>gd-ice</artifactId>
    <packaging>war</packaging>
    <version>3.6.3</version>
    <name>gd-ice</name>
    <description>Inventory of Composable Elements (ICE) for Synthetic Biology</description>
    <issueManagement>
        <system>Google Code</system>
        <url>http://code.google.com/p/gd-ice/issues/list</url>
    </issueManagement>
    <organization>
        <name>Joint BioEnergy Institute</name>
        <url>http://code.google.com/p/gd-ice/</url>
    </organization>
    <mailingLists>
        <mailingList>
            <name>gd-ice mailing list</name>
            <archive>http://groups.google.com/group/gd-ice</archive>
        </mailingList>
    </mailingLists>
    <scm>
        <connection>scm:git:https://code.google.com/p/gd-ice/</connection>
        <developerConnection>scm:git:https://code.google.com/p/gd-ice/</developerConnection>
        <url>http://code.google.com/p/gd-ice/source/checkout</url>
    </scm>
    <licenses>
        <license>
            <name>The new BSD License</name>
            <url>http://code.google.com/p/gd-ice/source/browse/LICENSE.txt</url>
            <distribution>manual</distribution>
        </license>
    </licenses>
    <dependencies>
