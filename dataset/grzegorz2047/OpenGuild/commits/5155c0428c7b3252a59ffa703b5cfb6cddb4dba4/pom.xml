<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>pl.grzegorz2047</groupId>
  <artifactId>OpenGuild2047</artifactId>
  <version>0.6.5</version>
  <packaging>jar</packaging>
  <name>OpenGuild2047</name>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <upload.dbo.category>server-mods</upload.dbo.category>
    <upload.dbo.slug>OpenGuild2047</upload.dbo.slug>
  </properties>
  <url>http://maven.apache.org</url>
  <ciManagement>
    <system>hudson</system>
    <url>http://5.231.53.149:8080/job/OpenGuild/</url>
  </ciManagement>
  <build>
    <resources>
        <resource>
            <directory>src/main/resources</directory>
            <filtering>true</filtering>
        </resource>
    </resources>
    <plugins>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-compiler-plugin</artifactId>
            <version>2.3.2</version>
            <configuration>
                <source>1.6</source>
                <target>1.6</target>
                <archive>
                    <addMavenDescriptor>false</addMavenDescriptor>
                </archive>
                <finalName>${project.name}</finalName>
            </configuration>
        </plugin>
    </plugins>
</build>
 <repositories>
  <repository>
    <id>bukkit-repo</id>
    <url>http://maven.sk89q.com/repo/</url>
  </repository>
  <repository>
    <id>njol-repo</id>
    <url>http://maven.njol.ch/repo/</url>
  </repository>
</repositories>
  <dependencies>
    <dependency>
      <groupId>org.bukkit</groupId>
      <artifactId>bukkit</artifactId>
      <version>1.7.10-R0.1-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>ch.njol</groupId>
      <artifactId>skript</artifactId>
      <version>2.0.1</version><!-- Use whatever version you want to depend on -->
      <scope>provided</scope>
    </dependency>
  </dependencies>
</project>
