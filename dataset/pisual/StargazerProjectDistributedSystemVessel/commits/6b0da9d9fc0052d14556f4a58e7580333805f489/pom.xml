	<build>
		<sourceDirectory>src</sourceDirectory>
		<resources>
			<resource>
				<directory>src</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
				<resource>
					<targetPath>${basedir}/WebRoot/WEB-INF/lib/</targetPath>
					<directory>${basedir}/WebRoot/WEB-INF/lib/</directory>
					<includes>
						<include>${basedir}/WebRoot/WEB-INF/lib/bigmemory-go-samples-4.3.3.1.9.jar</include>
						<include>${basedir}/WebRoot/WEB-INF/lib/ehcache-ee-2.10.3.1.12.jar</include>
						<include>${basedir}/WebRoot/WEB-INF/lib/hystrix-core-1.5.11.jar</include>
					</includes>
				</resource>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<warSourceDirectory>${basedir}/WebRoot</warSourceDirectory>
					<version>3.1</version>
					<failOnMissingWebXml>false</failOnMissingWebXml>
				</configuration>
			</plugin>
		</plugins>
