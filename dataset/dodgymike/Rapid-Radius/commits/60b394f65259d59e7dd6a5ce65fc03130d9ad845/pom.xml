<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.rapidradius</groupId>
	<artifactId>rapidradius</artifactId>
	<name>Rapid Radius Java Radius Library</name>
	<version>SNAPSHOT</version>
	<url>https://github.com/dodgymike/Rapid-Radius</url>
	<developers>
		<developer>
			<id>dodgymike</id>
			<name>Mike Davis</name>
			<email>dodgymike+rr@gmail.com</email>
			<timezone>GMT+2</timezone>
		</developer>
		<developer>
			<id>wuttke</id>
			<name>Matthias Wuttke</name>
			<email>mw@teuto.net</email>
			<timezone>CEST</timezone>
		</developer>
	</developers>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>JASIG</id>
			<name>JASIG</name>
			<url>http://developer.jasig.org/repo/content/groups/m2-legacy/</url>
		</repository>
	</repositories>
	<dependencies>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.1</version>
		</dependency>
		<dependency>
			<groupId>com.entersectmobile</groupId>
			<artifactId>utilj2me</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>org.gnu</groupId>
			<artifactId>gnu-crypto</artifactId>
			<version>2.0.1</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.2</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>
