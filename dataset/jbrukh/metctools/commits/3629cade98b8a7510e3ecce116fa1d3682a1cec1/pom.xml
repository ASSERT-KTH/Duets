<?xml version="1.0" encoding="UTF-8" ?>

<project
  xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation=
  "http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.marketcetera</groupId>
    <artifactId>source</artifactId>
    <version>1.6.0</version>
  </parent>
  
  <groupId>org.kohera</groupId>
  <artifactId>metctools</artifactId>
  <version>1.1.10-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>Kohera Metctools</name>

  <properties>
    <mvn.marketcetera.version>1.6.0</mvn.marketcetera.version>
  </properties>

   <dependencies>

    <!-- Compile. -->
    <dependency>
      <groupId>${mvn.public.package}</groupId>
      <artifactId>core</artifactId>
	  <version>[${mvn.marketcetera.version}]</version>
    </dependency>
	
    <dependency>
      <groupId>${mvn.public.package}</groupId>
      <artifactId>util</artifactId>
	  <version>[${mvn.marketcetera.version}]</version>
    </dependency>
	
	<dependency>
      <groupId>${mvn.public.package}</groupId>
      <artifactId>strategy</artifactId>
	  <version>[${mvn.marketcetera.version}]</version>
    </dependency>
	
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>[1.2.13,]</version>
    </dependency>
	
    <dependency>
      <groupId>javax.activation</groupId>
      <artifactId>activation</artifactId>
      <version>1.1</version>
    </dependency>
	
    <dependency>
      <groupId>javax.mail</groupId>
      <artifactId>mail</artifactId>
      <version>1.4</version>
    </dependency>
	
  </dependencies>
</project>
  
