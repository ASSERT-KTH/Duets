  <properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<jfinal-version>2.2</jfinal-version>
		
		<rest-version>1.1.0</rest-version>
		
		<mysql-version>5.1.20</mysql-version>
		<druid-version>1.0.18</druid-version>
