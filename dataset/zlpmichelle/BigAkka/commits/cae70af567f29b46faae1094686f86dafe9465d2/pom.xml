	<build>  
	
		<plugins>
		    <plugin>  
            <groupId>org.codehaus.mojo</groupId>  
            <artifactId>build-helper-maven-plugin</artifactId>  
            <version>1.1</version>  
            <executions>  
              <execution>  
                <id>add-source</id>  
                <phase>generate-sources</phase>  
                <goals>  
                  <goal>add-source</goal>  
                </goals>  
                <configuration>  
                  <sources>
                      <source>src</source>
                  </sources>
                </configuration>  
              </execution>  
            </executions>  
          </plugin>  
        </plugins>
	
	
		<resources>
			<resource>
