				<directory>conf</directory>
				<includes>
					<include>*.xml</include>
				</includes>
			</resource>
			
			<resource>
				<directory>.</directory>
				<includes>
					<include>lib/**/*.jar</include>
				</includes>
			</resource>
		</resources>
