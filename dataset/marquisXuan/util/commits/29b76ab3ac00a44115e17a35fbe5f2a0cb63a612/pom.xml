<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
         http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.yyx</groupId>
    <artifactId>util</artifactId>
    <version>1.0.7</version>

    <!--region Maven私服-->
    <repositories>
        <repository>
            <id>LenovoImageNexus</id>
            <name>Lenovo-Image</name>
            <url>http://maven.lenovoimage.com/repository/lenovoimage-release/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>
    <!-- region 用于jar发布的配置 -->
    <distributionManagement>
        <snapshotRepository>
            <id>Lenovo-Image-Nexus</id>
            <name>LenovoImage Nexus Snapshot</name>
            <url>http://maven.lenovoimage.com/repository/lenovoimage-snapshot/</url>
        </snapshotRepository>
        <repository>
            <id>Lenovo-Image-Nexus</id>
            <name>LenovoImage Nexus Release</name>
            <url>http://maven.lenovoimage.com/repository/lenovoimage-release/</url>
        </repository>
    </distributionManagement>
    <!-- endregion -->
    <!--endregion-->

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <poi>3.17</poi>
        <pdf>4.1.1</pdf>
        <httpclient>4.5.3</httpclient>
        <slf4j>1.7.25</slf4j>
        <logback>1.1.3</logback>
        <commons.logging>1.2</commons.logging>
        <commons.lang>3.7</commons.lang>
        <spring.boot.web>1.5.2.RELEASE</spring.boot.web>
        <fastJson>1.2.33</fastJson>
        <jodaTime>2.9.9</jodaTime>
        <mybatis>3.4.5</mybatis>
        <dom4j>1.6.1</dom4j>
        <junit>4.12</junit>
        <lombok>1.16.20</lombok>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.7.0</version>
                <configuration>
                    <encoding>utf-8</encoding>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.1</version>
                <configuration>
                    <attach>true</attach>
                </configuration>
                <executions>
                    <execution>
                        <!-- 指定绑定的声明周期阶段 -->
                        <phase>compile</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <!-- region spring boot starter web -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
            <version>${spring.boot.web}</version>
            <exclusions>
                <exclusion>
                    <artifactId>logback-classic</artifactId>
                    <groupId>ch.qos.logback</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>slf4j-api</artifactId>
                    <groupId>org.slf4j</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <!-- endregion -->
        <!-- region ice pdf core -->
        <dependency>
            <groupId>org.icepdf</groupId>
            <artifactId>icepdf-core</artifactId>
            <version>${pdf}</version>
        </dependency>
        <!-- endregion -->
        <!-- region httpclient -->
        <dependency>
            <groupId>org.apache.httpcomponents</groupId>
            <artifactId>httpclient</artifactId>
            <version>${httpclient}</version>
        </dependency>
        <!-- endregion -->
        <!-- region dom4j xml parse -->
        <dependency>
            <groupId>dom4j</groupId>
            <artifactId>dom4j</artifactId>
            <version>${dom4j}</version>
        </dependency>
        <!-- endregion -->
        <!-- region junit -->
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>${junit}</version>
            <scope>test</scope>
        </dependency>
        <!-- endregion -->
        <!-- region sfl4j -->
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <version>${slf4j}</version>
        </dependency>
        <!-- endregion -->
        <!-- region logback 1.3 -->
        <dependency>
            <groupId>ch.qos.logback</groupId>
            <artifactId>logback-classic</artifactId>
            <version>${logback}</version>
            <exclusions>
                <exclusion>
                    <artifactId>slf4j-api</artifactId>
                    <groupId>org.slf4j</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <!-- endregion -->
        <!-- region commons - logging  -->
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>${commons.logging}</version>
        </dependency>
        <!-- endregion -->
        <!-- region commons - lang3  -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>${commons.lang}</version>
        </dependency>
        <!-- endregion -->
        <!-- region poi -->
        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi</artifactId>
            <version>${poi}</version>
            <exclusions>
                <exclusion>
                    <artifactId>commons-codec</artifactId>
                    <groupId>commons-codec</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <!-- https://mvnrepository.com/artifact/org.apache.poi/poi-ooxml -->
        <dependency>
            <groupId>org.apache.poi</groupId>
            <artifactId>poi-ooxml</artifactId>
            <version>${poi}</version>
        </dependency>
        <!-- endregion -->
        <!-- region fastJson -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
            <version>${fastJson}</version>
        </dependency>
        <!-- endregion -->
        <!-- region joda Time -->
        <dependency>
            <groupId>joda-time</groupId>
            <artifactId>joda-time</artifactId>
            <version>${jodaTime}</version>
        </dependency>
        <dependency>
            <groupId>org.aspectj</groupId>
            <artifactId>aspectjrt</artifactId>
            <version>1.8.10</version>
            <scope>test</scope>
        </dependency>
        <!-- endregion -->
        <!-- region lombok -->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>${lombok}</version>
            <scope>provided</scope>
        </dependency>
        <!-- endregion -->
    </dependencies>

</project>