<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.xel</groupId>
  <artifactId>core</artifactId>
  <packaging>pom</packaging>
  <version>1.0</version>
  <name>core</name>
  <url>http://maven.apache.org</url>

    <modules>
        <module>externals/epl-to-c</module>
        <module>externals/java-c-interpreter</module>
    </modules>

    <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>

        <resources>
        <resource>
        <directory>conf</directory>
        <includes>
            <include>logging-default.properties</include>
            <include>nxt-default.properties</include>
            <include>nxt-junit-default.properties</include>
            <include>testvm.properties</include>
        </includes>
        </resource>
        </resources>
  </build>

