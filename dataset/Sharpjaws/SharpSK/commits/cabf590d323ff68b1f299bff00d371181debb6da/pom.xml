<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>me.sharpjaws.SharpSK</groupId>
	<artifactId>SharpSK</artifactId>
	<version>1.6.1.5-SNAPSHOT</version>
	<name>SharpSK</name>
	<description>A addon made for automating your favourite plugins.</description>
	<url>https://github.com/Sharpjaws/SharpSK</url>
	<packaging>jar</packaging>


	<repositories>
		<repository>
			<id>spigot-repo</id>
			<url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
		</repository>

		<repository>
			<id>njol-repo</id>
			<url>https://raw.githubusercontent.com/bensku/mvn-repo/master/</url>
		</repository>

		<repository>
			<id>pex-repo</id>
			<url>http://pex-repo.aoeu.xyz</url>
		</repository>

		<repository>
			<id>vault-repo</id>
			<url>http://maven.elmakers.com/repository/</url>
		</repository>


		<repository>
			<id>mcMMO-repo</id>
			<url>http://betonquest.betoncraft.pl/mvn/</url>
		</repository>

		<repository>
			<id>uCars-repo</id>
			<url>http://stormdev.org/jenkins/plugin/repository/project/uCars/LastSuccessful/repository/</url>
		</repository>

		<repository>
			<id>dmulloy2-repo</id>
			<url>http://repo.dmulloy2.net/content/groups/public/</url>
		</repository>

		<repository>
			<id>inventive-repo</id>
			<url>https://repo.inventivetalent.org/content/groups/public/</url>
		</repository>

		<repository>
			<id>authme-repo</id>
			<url>http://ci.xephi.fr/plugin/repository/everything/</url>
		</repository>

		<repository>
			<id>multiverse-repo</id>
			<url>http://repo.onarandombox.com/content/repositories/multiverse-snapshots/</url>
		</repository>
	</repositories>




	<build>
		<sourceDirectory>src/main/java</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-clean-plugin</artifactId>
				<version>3.0.0</version>
				<configuration>
					<failOnError>false</failOnError>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.5.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<failOnError>false</failOnError>
				</configuration>
			</plugin>
		</plugins>
	</build>



	<dependencies>

		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.10</version>
			<scope>test</scope>
		</dependency>
	

		<dependency>
			<groupId>org.spigotmc</groupId>
			<artifactId>spigot-api</artifactId>
			<version>1.11.2-R0.1-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>ch.njol</groupId>
			<artifactId>Skript</artifactId>
			<version>2.2-dev22b</version>
		</dependency>


		<dependency>
			<groupId>ru.tehkode</groupId>
			<artifactId>PermissionsEx</artifactId>
			<version>1.23.2-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>net.elseland.xikage.MythicMobs</groupId>
			<artifactId>mythicmobs</artifactId>
			<version>4.0.1</version>
		</dependency>


		<dependency>
			<groupId>com.gmail.nossr50</groupId>
			<artifactId>mcmmo</artifactId>
			<version>1.5.04-SNAPSHOT</version>
		</dependency>


		<dependency>
			<groupId>org.inventivetalent</groupId>
			<artifactId>glowapi</artifactId>
			<version>1.4.4</version>
		</dependency>

		<dependency>
			<groupId>fr.xephi</groupId>
			<artifactId>authme</artifactId>
			<version>5.2-SNAPSHOT</version>
		</dependency>


	</dependencies>


	<properties>
		<project.build.sourceEncoding>CP1252</project.build.sourceEncoding>
	</properties>
</project>

