<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>se.jayway.maven.plugins</groupId>
	<artifactId>maven-lab-plugin</artifactId>
	<packaging>maven-plugin</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>maven-lab-plugin Maven Mojo</name>
	<url>http://maven.apache.org</url>
	<description>
        Maven plugin used to create labs that provide more code once a step has been completed.
        Labs are a useful way to learn new technologies, frameworks and tools.
    </description>
	<inceptionYear>2009</inceptionYear>
	<licenses>
		<license>
			<name>The Apache Software License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<developers>
		<developer>
			<id>jan.kronquist</id>
			<name>Jan Kronquist</name>
			<organization>Jayway</organization>
			<organizationUrl>http://www.jayway.se</organizationUrl>
			<email>jan.kronquist at jayway.se</email>
			<timezone>+1</timezone>
		</developer>
	</developers>
	<distributionManagement>
		<snapshotRepository>
			<uniqueVersion>true</uniqueVersion>
			<id>jayway-snapshot-repo</id>
			<name>Jayway Public Snapshot Repository</name>
			<url>http://ec2-79-125-38-163.eu-west-1.compute.amazonaws.com/nexus/content/repositories/snapshots/</url>
			<layout>default</layout>
		</snapshotRepository>
		<site>
			<id>local-site</id>
			<name>Local Site</name>
			<url>target/site</url>
		</site>
	</distributionManagement>
	<dependencies>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-plugin-api</artifactId>
			<version>2.0</version>
		</dependency>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-project</artifactId>
			<version>2.0.6</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.5</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-plugin-plugin</artifactId>
			</plugin>
		</plugins>
	</reporting>
</project>
