	<repositories>
	    <repository>
	        <id>mvn-repo</id>
	        <url>http://maven.nlpcn.org/</url>
	    </repository>
	</repositories>
	
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>findbugs-maven-plugin</artifactId>
				<version>2.5.1</version>
				<configuration>
					<xmlOutput>true</xmlOutput>
					<effort>Max</effort>
				</configuration>
			</plugin>
		</plugins>
	</reporting>

</project>