    <repositories>
        <repository>
            <id>shibboleth.net.releases</id>
            <name>shibboleth</name>
            <layout>default</layout>
            <url>https://build.shibboleth.net/nexus/content/repositories/releases</url>
            <releases>
                <updatePolicy>always</updatePolicy>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>shibboleth.net.public</id>
            <name>shibboleth</name>
            <layout>default</layout>
            <url>https://build.shibboleth.net/nexus/content/repositories/public</url>
            <releases>
                <updatePolicy>always</updatePolicy>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>shibboleth.net.thirdparty</id>
            <name>shibboleth</name>
            <layout>default</layout>
            <url>https://build.shibboleth.net/nexus/content/repositories/thirdparty</url>
            <releases>
                <updatePolicy>always</updatePolicy>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>

    <dependencies>
    	<dependency>
    		<groupId>javax.servlet</groupId>
    		<artifactId>servlet-api</artifactId>
    		<version>2.5</version>
    		<type>jar</type>
    		<scope>provided</scope>
    	</dependency>

        <dependency>
            <groupId>edu.internet2.middleware</groupId>
            <artifactId>shibboleth-identityprovider</artifactId>
            <version>2.4.0</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>
