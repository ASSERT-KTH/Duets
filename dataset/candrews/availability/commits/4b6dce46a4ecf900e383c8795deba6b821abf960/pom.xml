		    <version>2.1.20</version>
		</dependency>
		<dependency>
          <groupId>org.webjars.bower</groupId>
          <artifactId>fullcalendar</artifactId>
          <version>2.3.1</version>
		</dependency>
        <dependency>
          <groupId>nz.net.ultraq.thymeleaf</groupId>
          <artifactId>thymeleaf-layout-dialect</artifactId>
      	</dependency>
