<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.devdom</groupId>
    <artifactId>commons-do</artifactId>
    <version>0.8.2</version>
    <packaging>jar</packaging>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>1.5</maven.compiler.source>
        <maven.compiler.target>1.5</maven.compiler.target>
    </properties>
    <name>Comunes para Developers Dominicanos</name>
    <organization>
        <name>Developers Dominicanos</name>
        <url>http://developers.do</url>
    </organization>
	<scm>
        <url>https://github.com/carlosvasquez/commons-do</url>
        <developerConnection>https://github.com/carlosvasquez/commons-do.git</developerConnection>
    </scm>
    <developers>
        <developer>
            <name>Carlos Vásquez Polanco</name>
            <email>cvasquezpolanco@gmail.com</email>
            <url>http://www.linkedin.com/in/carlosvasquez</url>
            <organization>Developers Dominicanos</organization>
            <organizationUrl>http://developers.do</organizationUrl>
            <properties>
                <project>common-validations</project>
            </properties>
        </developer>
    </developers>
    <dependencies>
        <dependency>
            <groupId>org.apache.httpcomponents</groupId>
            <artifactId>httpclient</artifactId>
            <version>4.3.5</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.json</groupId>
            <artifactId>json</artifactId>
            <version>20090211</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.10</version>
            <scope>test</scope>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
            <id>m2.java.net</id>
            <name>Java.net Maven 2 Repository</name>
            <layout>default</layout>
            <url>http://download.java.net/maven/2</url>            
        </repository>        
        <repository>
            <id>eclipselink</id>
            <layout>default</layout>
            <name>Repository for library EclipseLink (JPA 2.0)</name>
            <url>http://download.eclipse.org/rt/eclipselink/maven.repo/</url>
        </repository>        
        <repository>
            <id>jetty-releases</id>
            <name>Jetty Releases</name>
            <url>https://oss.sonatype.org/content/repositories/jetty-releases</url>
        </repository>        
    </repositories>
    <description>Inspirado por http://data.developers.do se creó un API para consumir la data expuesta en http://data.developers.do</description>
</project>