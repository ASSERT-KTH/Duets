<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>MemoryMeppedFiles</groupId>
  <artifactId>MemoryMeppedFiles</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  
  <dependencies>
  	<dependency>
    	<artifactId>ConciseSet</artifactId>
    	<groupId>it.uniroma3.mat.extendedset.intset.ConciseSet</groupId>
     	<version>2.2.0</version>
    	<scope>system</scope>
    	<systemPath>${basedir}/lib/extendedset2.2.jar</systemPath>
	</dependency>
	<dependency>
    	<artifactId>SizeOf</artifactId>
    	<groupId>net.sourceforge</groupId>
     	<version>1.0-SNAPSHOT</version>
    	<scope>system</scope>
    	<systemPath>${basedir}/lib/SizeOf.jar</systemPath>
	</dependency>
  	<dependency>
  		<groupId>com.google.guava</groupId>
		<artifactId>guava</artifactId>
		<version>17.0</version>
		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>org.hamcrest</groupId>
		<artifactId>hamcrest-core</artifactId>
		<version>1.3</version>
		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>junit</groupId>
		<artifactId>junit</artifactId>
		<version>4.11</version>
		<scope>compile</scope>
  	</dependency>
  	<dependency>
  		<groupId>com.google.code.findbugs</groupId>
		<artifactId>jsr305</artifactId>
		<version>2.0.3</version>
		<scope>compile</scope>
  	</dependency>
  	<dependency>
        <groupId>org.roaringbitmap</groupId>
        <artifactId>RoaringBitmap</artifactId>
        <version>0.2.2</version>
        <scope>compile</scope>
      </dependency>
  	<dependency>
  		<groupId>com.google.collections</groupId>
		<artifactId>google-collections</artifactId>
		<version>1.0</version>
		<scope>compile</scope>
  	</dependency>
  </dependencies>  
</project>